{
  "name": "my-corner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "DOTENV_CONFIG_PATH=.env.local NODE_ENV=development node -r dotenv/config node_modules/.bin/ts-node --project tsconfig.server.json src/server/index.ts",
    "dev:local": "NODE_ENV=development USE_SQLITE=true PORT=3001 PUBLIC_URL=http://localhost:3001 node node_modules/.bin/ts-node --project tsconfig.server.json src/server/index.ts",
    "dev:prod": "DOTENV_CONFIG_PATH=.env.prod node -r dotenv/config node_modules/.bin/ts-node --project tsconfig.server.json src/server/index.ts",
    "build": "next build && npm run build:server",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node dist/src/server/index.js",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "DOTENV_CONFIG_PATH=.env.prod node -r dotenv/config node_modules/.bin/drizzle-kit push",
    "db:studio": "DOTENV_CONFIG_PATH=.env.prod node -r dotenv/config node_modules/.bin/drizzle-kit studio",
    "db:studio:local": "drizzle-kit studio --config drizzle.config.local.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "build-docker": "docker build -t my-corner .",
    "start-docker": "docker run -p 3000:3000 --env-file .env.prod my-corner"
  },
  "dependencies": {
    "@apollo/client": "3.8.8",
    "@apollo/server": "^4.10.0",
    "@supabase/supabase-js": "^2.87.3",
    "better-sqlite3": "^12.5.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.17.3",
    "graphql": "^16.8.1",
    "multer": "^1.4.5-lts.1",
    "next": "14.0.4",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^13.0.0",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
